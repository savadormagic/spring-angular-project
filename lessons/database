Student(
    id serial,
    idd int,
    first_name varchar(50),
    middle_name varchar(50),
    last_name varchar(50),
    passport varchar(15),
    birth_date timestamp,
    create_date timestamp,
    delete_date timestamp,
    status varchar(10)
)

Teacher(
    id serial primary key,
    idd int not null,
    first_name varchar(50),
    middle_name varchar(50),
    last_name varchar(50),
    passport varchar(15),
    birth_date timestamp(0),
    create_date timestamp(0) not null,
    delete_date timestamp(0),
    status varchar(10)
)

USER (
    id serial primary key,
    idd int not null,
    first_name varchar(50),
    middle_name varchar(50),
    last_name varchar(50),
    passport varchar(15),
    birth_date timestamp(0),
    create_date timestamp(0) not null,
    delete_date timestamp(0),
    status varchar(10)
    )

Course(
    id serial,
    idd int,
    name varchar(100),
    description varchar(500),
    teacher_idd int,
    max_count_student int,
    start_date timestamp,
    end_date timestamp,
    create_date timestamp,
    delete_date timestamp,
    status varchar(10)
)
Student_to_course(
    id serial,
    student_idd int,
    course_idd int,
    success boolean
)

Room(
    id serial,
    idd int,
    number varchar(5),
    block varchar(5),
    max_students int,
    create_date timestamp,
    delete_date timestamp
)

Lesson(
    id serial,
    name varchar(100),
    description varchar(1000),
    course_idd int,
    room_idd int,
    lesson_date_start timestamp,
    lesson_date_end timestamp,
    extra_instruments varchar(1000) //PRINTER,SENSOR_BOARD,...
)

INSTRUMENT_TO_LESSON (
    id serial primary key,
    instrument_idd int references INSTRUMENT (idd),
    lesson_idd int references LESSON (idd)
)

INSTRUMENT_TO_ROOM (
    id serial primary key,
    instrument_idd int references INSTRUMENT (idd),
    room_idd int references ROOM (idd)
)

Instrument(
    id serial
    idd int,
    name varchar(50),
    number varchar(),
    create_date timestamp,
    delete_date timestamp
)